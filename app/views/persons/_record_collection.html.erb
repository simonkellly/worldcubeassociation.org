<% if @enable_react %>
  <%= react_component("Persons/RecordCollection", {
    wr: @records[:world] > 0 ? @records[:world] : nil,
    cr: @records[:continental] > 0 ? @records[:continental] : nil,
    nr: @records[:national] > 0 ? @records[:national] : nil,
  }) %>
<% else %>
  <%= render "record_collection_legacy" %>
<% end %>
